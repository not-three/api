{
  "name": "@not3/api",
  "version": "IN-DEV",
  "description": "!3 is a simple, secure and open source paste sharing platform.",
  "author": "Joschua Becker EDV <support@scolasti.co>",
  "private": true,
  "license": "AGPL-3.0-only",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "nest start --watch",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "build": "npm run build:nest && npm run build:docs",
    "build:nest": "nest build",
    "build:docs": "typedoc",
    "pg:dev": "pnpm pg:reset && pnpm pg:build && pnpm pg:start",
    "pg:base": "docker compose -f docker-compose.db.yml -f docker-compose.override.yml",
    "pg:build": "pnpm pg:base build",
    "pg:start": "pnpm pg:base up",
    "pg:reset": "pnpm pg:base down --volumes"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.726.1",
    "@aws-sdk/s3-presigned-post": "^3.726.1",
    "@aws-sdk/s3-request-presigner": "^3.726.1",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^8.1.0",
    "cache-manager": "^5.0.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.7",
    "fix-esm": "^1.0.1",
    "knex": "^3.1.0",
    "mysql": "^2.18.1",
    "nanoid": "^5.0.9",
    "p-retry": "^6.2.1",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.7",
    "typedoc": "^0.27.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
